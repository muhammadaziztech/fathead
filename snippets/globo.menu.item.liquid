            <div class="gm-grid-item gmcol-6">
              {%- assign namePlaceholder = menuItem.name | default: settings.language.name -%}
              {%- assign emailPlaceholder = menuItem.email | default: settings.language.email -%}
              {%- assign phonePlaceholder = menuItem.phone_number | default: settings.language.phone_number -%}
              {%- assign messagePlaceholder = menuItem.message | default: settings.language.message -%}
              {%- assign sendPlaceholder = menuItem.send | default: settings.language.send -%}
              {%- if languageCode != "" -%}
                {%- assign nameLocale = 'name' | append: languageCode -%}
                {%- if menuItem[nameLocale] -%}
                {%- assign namePlaceholder = menuItem[nameLocale] -%}
                {%- endif -%}
                {%- assign emailLocale = 'email' | append: languageCode -%}
                {%- if menuItem[emailLocale] -%}
                {%- assign emailPlaceholder = menuItem[emailLocale] -%}
                {%- endif -%}
                {%- assign phoneLocale = 'phone_number' | append: languageCode -%}
                {%- if menuItem[phoneLocale] -%}
                {%- assign phonePlaceholder = menuItem[phoneLocale] -%}
                {%- endif -%}
                {%- assign messageLocale = 'message' | append: languageCode -%}
                {%- if menuItem[messageLocale] -%}
                {%- assign messagePlaceholder = menuItem[messageLocale] -%}
                {%- endif -%}
                {%- assign sendLocale = 'send' | append: languageCode -%}
                {%- if menuItem[sendLocale] -%}
                {%- assign sendPlaceholder = menuItem[sendLocale] -%}
                {%- endif -%}
              {%- endif -%}
