{% assign showMenuItem = true %}
{% if menuItem.schedulePublishing %}
{% assign now = "now" | date: "%s" | plus: 0 %}

{{menuItem.fromDate | replace: "T", " " | append: ":00"}} - {{"now" | date: "%Y-%m-%d %H:%M:00"}} - {{menuItem.to_date | replace: "T", " " | append: ":00"}}
{% assign publishingFrom = menuItem.fromDate | replace: "T", " " | append: ":00" | date: "%s" | plus: 0 %}
{% assign publishingTo = menuItem.toDate | replace: "T", " " | append: ":00" | date: "%s" | plus: 0 %}
{{publishingFrom}} - {{now}} - {{publishingTo}}
{% if publishingFrom > 0 and now < publishingFrom %}{% assign showMenuItem = false %}{% endif %}
{% if publishingTo > 0 and now > publishingTo %}{% assign showMenuItem = false %}{% endif %}
{% endif %}

{% if showMenuItem %}

{% assign widths = "165,360,533,720,1000,1500" | split: "," %}
{% assign languageCode = "" %}
{%- assign shopLocale = 'null' -%}
{%- for locale in shop.published_locales -%}
  {%- if locale.primary -%}
    {%- assign shopLocale = locale.iso_code -%}
  {%- endif -%}
{%- endfor -%}
{% if request.locale.iso_code != shopLocale %}
{% assign languageCode = "_" | append: request.locale.iso_code %}
{% endif %}
{% capture menuItemClass %}gm-item{% if menuItem.hideOnDesktop %} gm-hidden-desktop{% endif %}{% if menuItem.hideOnMobile %} gm-hidden-mobile{% endif %}{% if menuItem.noPadding %} gm-no-padding{% endif %}{% if menuItem.level and menuItem.level == 0 %} gm-level-{{ menuItem.level }}{% endif %}{% if menuItem.width %} gm-grid-item gmcol-{{ menuItem.width }}{% endif %}{% if menuItem.is_heading or menuItem.submenu.type == 'products-list' %} gm-heading{% endif %}{% if menuItem.submenu.items.size > 0 %} gm-has-submenu{% if menuItem.submenu.type and menuItem.submenu.type != "tabContents" %} gm-submenu-{{ menuItem.submenu.type }}{% endif %}{% if menuItem.submenu.alignment %}{% if menuItem.submenu.type == "dropdown" and menuItem.submenu.orientation == "horizontal" %} gm-submenu-align-full{% else %} gm-submenu-align-{{ menuItem.submenu.alignment }}{% endif %}{% endif %}{% endif %}{% if menuItem.active %} gm-active{% endif %}{% if menuItem.class %} {{ menuItem.class }}{% endif %}{% if menuItem.extraClass %} {{ menuItem.extraClass }}{% endif %}{% endcapture %}

{% assign menuItemTitle = menuItem.title %}
{% assign menuItemDescription = menuItem.description %}
{% assign menuItemImage = menuItem.image %}
{% assign menuItemBadgeTitle = menuItem.badge.title %}
{% assign menuItemIcon = menuItem.icon %}
{% assign menuItemUrl = menuItem.url %}
{% assign menuItemUrlTag = menuItem.urlTag %}

{% if languageCode != "" %}
  {% assign titleLanguageCode = 'title' | append: languageCode %}
  {% if menuItem[titleLanguageCode] %}
  {% assign menuItemTitle = menuItem[titleLanguageCode] %}
  {% endif %}
  {% assign descriptionLanguageCode = 'description' | append: languageCode %}
  {% if menuItem[descriptionLanguageCode] %}
  {% assign menuItemDescription = menuItem[descriptionLanguageCode] %}
  {% endif %}
  {% assign imageLanguageCode = 'image' | append: languageCode %}
  {% if menuItem[imageLanguageCode] %}
  {% assign menuItemImage = menuItem[imageLanguageCode] %}
  {% endif %}
  {% assign badgeTitleLanguageCode = 'title' | append: languageCode %}
  {% if menuItem[badgeTitleLanguageCode] %}
  {% assign menuItemBadgeTitle = menuItem.badge[badgeTitleLanguageCode] %}
  {% endif %}
  {% assign iconLanguageCode = 'icon' | append: languageCode %}
  {% if menuItem[iconLanguageCode] %}
  {% assign menuItemIcon = menuItem[iconLanguageCode] %}
  {% endif %}
  {% assign urlLanguageCode = 'url' | append: languageCode %}
  {% if menuItem[urlLanguageCode] %}
  {% assign menuItemUrl = menuItem[urlLanguageCode] %}
  {% endif %}
  {% assign urlTagLanguageCode = 'urlTag' | append: languageCode %}
  {% if menuItem[urlTagLanguageCode] %}
  {% assign menuItemUrlTag = menuItem[urlTagLanguageCode] %}
  {% endif %}
{% endif %}

{% if menuItemUrl and menuItemUrlTag and menuItemUrl contains '/collections/' %}
  {% assign menuItemUrlTag = menuItemUrlTag | split: ',' %}
  {% capture menuItemUrl %}{{ menuItemUrl }}/{% for tag in menuItemUrlTag %}{{ tag | strip | replace: " ", "-" | url_encode }}{% unless forloop.last %}+{% endunless %}{% endfor %}{% endcapture %}
{% endif %}

{% assign addToCartText = settings.language.add_to_cart_default | default: settings.language.add_to_cart %}
{% assign soldOutText = settings.language.sold_out_default | default: settings.language.sold_out %}
{% assign saleText = settings.language.sale_default | default: settings.language.sale %}
{% if languageCode != "" %}
  {% assign atcLanguageCode = 'add_to_cart' | append: languageCode %}
  {% if settings.language[atcLanguageCode] %}
  {% assign addToCartText = settings.language[atcLanguageCode] %}
  {% endif %}
  {% assign soldLanguageCode = 'sold_out' | append: languageCode %}
  {% if settings.language[soldLanguageCode] %}
  {% assign soldOutText = settings.language[soldLanguageCode] %}
  {% endif %}
  {% assign saleLanguageCode = 'sale' | append: languageCode %}
  {% if settings.language[saleLanguageCode] %}
  {% assign saleText = settings.language[saleLanguageCode] %}
  {% endif %}
{% endif %}

{% case menuItem.type %}
  {% when 'links' %}
    <li class="{{ menuItemClass }}">
      <ul class="gm-links{% if menuItem.justifyContent %}  gm-jc-{{ menuItem.justifyContent }}{% endif %}">
        {% assign linkGroup = 0 %}
        {% for item in menuItem.submenu.items %}
          {% if item.is_heading %}{% assign linkGroup = linkGroup | plus: 1 %}{% endif %}
          {% render 'globo.menu.item', menuItem: item, linkGroup: linkGroup %}
        {% endfor %}
      </ul>
    </li>
  {% when 'tabs' %}
    <li class="{{ menuItemClass }}">
      <div class="gm-tabs gm-tabs-{{ menuItem.alignment }} {% if menuItem.alignment != 'top' and menuItem.submenu.alignment %}gm-jc-{{ menuItem.submenu.alignment }}{% endif %}">
        <ul class="gm-tab-links{% if menuItem.alignment == 'top' and menuItem.submenu.alignment %}gm-jc-{{ menuItem.submenu.alignment }}{% endif %}">
          {% for item in menuItem.submenu.items %}
            {% render 'globo.menu.item', menuItem: item, dataTabIndex: forloop.index0, noSubMenu: true %}
          {% endfor %}
        </ul>
        <ul class="gm-tab-contents">
          {% for item in menuItem.submenu.items %}
          <li data-tab-index="{{ forloop.index0 }}" class="gm-tab-content{% if item.submenu.items.size > 0 %} hc{% endif %}{% if item.active %} gm-active{% endif %}">
            <ul class="gm-tab-panel">
              {% for subItem in item.submenu.items %}
                {% render 'globo.menu.item', menuItem: subItem %}
              {% endfor %}
              {%- unless shop.metafields.globo_menu.remove_watermark %}
              {%- if aliExpress %}<p style="text-align:right;display:flex !important;flex:0 0 100%;font-size:11px;padding:3px;margin:0;justify-content:flex-end;opacity:0.7;background:transparent">Powered by <a style="margin: 0 0 0 3px !important;padding: 0 !important;color: #5c6ac4 !important;font-weight: bold !important;font-size: 12px !important;" href="https://apps.shopify.com/globo-mega-menu">Globo Mega Menu</a></p>{% endif -%}
              {% endunless -%}
            </ul>
          </li>
          {% endfor %}
        </ul>
      </div>
    </li>
  {% when 'image' %}
   {%- capture imageWidth -%}--gm-item-image-width:{%- if menuItem.image_width -%}{{- menuItem.image_width -}}{%- else -%}{{- "auto" -}}{%- endif -%}{%- endcapture -%}
    <li class="{{ menuItemClass }}">
      <div class="gm-image" {% if imageWidth != "" %} style="{{- imageWidth -}}"{% endif %}>
        <a title="{{ menuItemTitle | escape }}"{% if menuItem.targetBlank %} target='_blank'{% endif %} href="{{ menuItemUrl }}">
          {%- if menuItemImage -%} 
            <img
              {% if settings.general.lazy_load_enable %}srcSet="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw=="{% endif %}
              data-sizes="auto"
              {% if settings.general.lazy_load_enable %}data-src="{{ menuItemImage | file_img_url: 'large' | replace: '_large.', imgLz | replace: '.JPG', '.jpg' | replace: '.jpeg', '.jpg' }}"{% else %}src="{{ menuItemImage | file_img_url: '2048x' | replace: '.JPG', '.jpg' | replace: '.jpeg', '.jpg' }}"{% endif %}
              width="{{ menuItemImage.width }}"
              height="{{ menuItemImage.height }}"
              loading="lazy"
              data-widths="[40, 100, 140, 180, 250, 260, 275, 305, 440, 610, 720, 930, 1080]"
              class="{% if settings.general.lazy_load_enable %}gmlazyload {% endif %}{% if class %} {{ class }}{% endif %}"
              alt="{{ menuItemTitle | escape }}"
            />
          {%- endif -%}
          {%- if menuItemTitle or menuItemBadgeTitle or menuItemDescription -%}
          <div class="gm-target{% if menuItem.justifyContent %} gm-jc-{{ menuItem.justifyContent }}{% endif %}">
            {% assign text = menuItemTitle | split: "-" %}
            <div class="snize">
              <div class="snize-title-line1">{{ text[0] }}</div>
              <div class="snize-title-line2">{{ text[1] }}</div>
              <div class="snize-title-line3">{{ text[2] }}</div>
            </div>
            {% comment %}
              <span class="gm-text">
                {%- if menuItemBadgeEnable and menuItemBadgeTitle -%}
                <span class="gm-badge gm-badge-{% if menuItem.level == 0 %}top{% else %}left{% endif %}" style="color: {{ menuItem.badge.backgroundColor }}">
                  <span style="background: {{ menuItem.badge.backgroundColor }}; color: {{ menuItem.badge.color }}">{{ menuItemBadgeTitle }}</span>
                </span>
                {%- endif -%}
                {%- if menuItemDescription -%}<span class="gm-description">{{ menuItemDescription }}</span>{%- endif -%}
              </span>
            {% endcomment %}
          </div>
          {%- endif -%}
        </a>
      </div>
    </li>
  {% when 'product' %}
    {% assign pClass = "" %}
    {% if menuItem.display == "image_left" %}
      {% assign pClass = "gm-single-product" %}
    {% endif %}
    <li class="{{ menuItemClass }}">
      {% render 'globo.menu.item.product', menuItem: item, layout: menuItem.display, handle: menuItem.product.handle, class: pClass, saleText: saleText, soldOutText: soldOutText, addToCartText: addToCartText %}
    </li>
  {% when 'products' %}
    {% if menuItem.view == "carousel" %}
      {% assign carouselMaxItem = 4 %}
      {% if menuItem.itemWidth > 0 %}
        {% assign carouselMaxItem = 12 | divided_by: menuItem.itemWidth %}
      {% endif %}
      <li class="{{ menuItemClass }}">
        <ul class="gm-products-carousel{% if menuItemTitle %} no-heading{% endif %}">
          {% if menuItemTitle %}
          <li class="gm-heading">
            <a class="gm-target"{% if menuItem.targetBlank %} target="_blank"{% endif %} href="{{ menuItemUrl }}">{{menuItemTitle}}</a>
          </li>
          {% endif %}
          <li id="gm-owlcarousel-outer" class="gm-owlcarousel-outer">
            <div class="gm-owlcarousel owl-carousel" data-show="{{ carouselMaxItem }}">
              {% if menuItem.products %}
                {% for product in menuItem.products %}
                  {% render 'globo.menu.item.product', menuItem: menuItem, layout: menuItem.display, handle: product.handle, saleText: saleText, soldOutText: soldOutText, addToCartText: addToCartText %}
                {% endfor %}
              {% endif %}
            </div>
          </li>
        </ul>
      </li>
    {% elsif menuItem.view == 'grid' %}
      <li class="{{ menuItemClass }}">
        <ul class="gm-products-grid gm-grid">
          {% if menuItemTitle %}
            <li class="gmcol-12">
              <div class="gm-heading">
                <a class="gm-target"{% if menuItem.targetBlank %} target="_blank"{% endif %} href="{{ menuItemUrl }}">{{menuItemTitle}}</a>
              </div>
            </li>
          {% endif %}
          {% for product in menuItem.products %}
            {% if menuItem.itemView == 'image_left' %}
              <li class="gm-grid-item {% if menuItem.itemWidth %} gmcol-{{ menuItem.itemWidth }}{% else %} gmcol{% endif %}">
                {% render 'globo.menu.item.product', menuItem: menuItem, layout: menuItem.itemView, handle: product.handle, saleText: saleText, soldOutText: soldOutText, addToCartText: addToCartText %}
              </li>
            {% else %}
              <li class="gm-grid-item {% if menuItem.itemWidth %} gmcol-{{ menuItem.itemWidth }}{% else %} gmcol{% endif %}">
                {% render 'globo.menu.item.product', menuItem: menuItem, layout: menuItem.itemView, handle: product.handle, saleText: saleText, soldOutText: soldOutText, addToCartText: addToCartText %}
              </li>
            {% endif %}
          {% endfor %}
        </ul>
      </li>
    {% else %}
      <li class="{{ menuItemClass }}">
        <ul class="gm-products-list gm-grid">
          {% if menuItemTitle %}
            <li class="gmcol-12">
              <div class="gm-heading">
                <a class="gm-target"{% if menuItem.targetBlank %} target="_blank"{% endif %} href="{{ menuItemUrl }}">{{menuItemTitle}}</a>
              </div>
            </li>
          {% endif %}
          {% for product in menuItem.products %}
            <li class="gm-grid-item gmcol-12">
              {% render 'globo.menu.item.product', menuItem: menuItem, layout: menuItem.layout, handle: product.handle, saleText: saleText, soldOutText: soldOutText, addToCartText: addToCartText %}
            </li>
          {% endfor %}
        </ul>
      </li>
    {% endif %}
  {% when 'articles' or 'blog' %}
    {% if menuItem.view == "carousel" %}
      {% assign carouselMaxItem = 4 %}
      {% if menuItem.itemWidth > 0 %}
        {% assign carouselMaxItem = 12 | divided_by: menuItem.itemWidth %}
      {% endif %}
      <li class="{{ menuItemClass }}">
        <ul class="gm-products-carousel{% if menuItemTitle %} no-heading{% endif %}">
          {% if menuItemTitle %}
          <li class="gm-heading">
            <a class="gm-target"{% if menuItem.targetBlank %} target="_blank"{% endif %} href="{{ menuItemUrl }}">{{ menuItemTitle }}</a>
          </li>
          {% endif %}
          <li id="gm-owlcarousel-outer" class="gm-owlcarousel-outer">
            <div class="gm-owlcarousel owl-carousel" data-show={{ carouselMaxItem }}>
              {% if menuItem.type == 'articles' and menuItem.articles %}
                {% for article in menuItem.articles %}
                  {% assign article = articles[article.handle] %}
                  {% render 'globo.menu.item.article', article: article, layout: menuItem.itemView %}
                {% endfor %}
              {% elsif menuItem.type == 'blog' and menuItem.blog.handle and blogs[menuItem.blog.handle].articles.size > 0 %}
                {% for article in blogs[menuItem.blog.handle].articles limit: menuItem.limit %}
                  {% render 'globo.menu.item.article', article: article, layout: menuItem.itemView %}
                {% endfor %}
              {% endif %}
            </div>
          </li>
        </ul>
      </li>
    {% elsif menuItem.view == 'grid' %}
      <li class="{{ menuItemClass }}">
        <ul class="gm-products-grid gm-grid">
          {% if menuItemTitle %}
            <li class="gmcol-12">
              <div class="gm-heading">
                <a class="gm-target"{% if menuItem.targetBlank %} target="_blank"{% endif %} href="{{ menuItemUrl }}">{{ menuItemTitle }}</a>
              </div>
            </li>
          {% endif %}
          {% if menuItem.type == 'articles' and menuItem.articles %}
            {% for article in menuItem.articles %}
              {% assign article = articles[article.handle] %}
              <li class="gm-grid-item{% if menuItem.itemWidth %} gmcol-{{ menuItem.itemWidth }}{% else %} gmcol{% endif %}">
                {% render 'globo.menu.item.article', article: article, layout: menuItem.itemView %}
              </li>
            {% endfor %}
          {% elsif menuItem.type == 'blog' and menuItem.blog.handle and blogs[menuItem.blog.handle].articles.size > 0 %}
            {% for article in blogs[menuItem.blog.handle].articles limit: menuItem.limit %}
              <li class="gm-grid-item{% if menuItem.itemWidth %} gmcol-{{ menuItem.itemWidth }}{% else %} gmcol{% endif %}">
                {% render 'globo.menu.item.article', article: article, layout: menuItem.itemView %}
              </li>
            {% endfor %}
          {% endif %}
        </ul>
      </li>
    {% else %}
      <li class="{{ menuItemClass }}">
        <ul class="gm-products-list gm-grid">
          {% if menuItemTitle %}
          <li class="gmcol-12">
            <div class="gm-heading">
              <a class="gm-target"{% if menuItem.targetBlank %} target="_blank"{% endif %} href="{{ menuItemUrl }}">{{ menuItemTitle }}</a>
            </div>
          </li>
          {% endif %}
          {% if menuItem.type == 'articles' and menuItem.articles %}
            {% for article in menuItem.articles %}
              {% assign article = articles[article.handle] %}
              <li class="gm-grid-item gmcol-12">
                {% render 'globo.menu.item.article', article: article, layout: 'image_left' %}
              </li>
            {% endfor %}
          {% elsif menuItem.type == 'blog' and menuItem.blog.handle and blogs[menuItem.blog.handle].articles.size > 0 %}
            {% for article in blogs[menuItem.blog.handle].articles limit: menuItem.limit %}
              <li class="gm-grid-item gmcol-12">
                {% render 'globo.menu.item.article', article: article, layout: 'image_left' %}
              </li>
            {% endfor %}
          {% endif %}
        </ul>
      </li>
    {% endif %}
  {% when 'collections' %}
    <li class="{{ menuItemClass }}">
      <div class="gm-collections gm-grid">
        {% for collection in menuItem.collections %}
          {% assign menuItemTitle = collection.title %}
          {% if collections[collection.handle] %}
            {% assign collectionLiquid = collections[collection.handle] %}
          {% endif %}
        {% endfor %}
        {% if menuItem.display == "image_left" %}
          <div class="gm-grid-item gm-collection gm-collection-left{% if menuItem.itemWidth %} gmcol-{{ menuItem.itemWidth }}{% else %} gmcol{% endif %}">
            <a class="gm-target" href="{{ collectionLiquid.url }}">
              <div class="gm-collection-img">
                {% if collectionLiquid.featured_image %}
                <img
                  srcSet="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw=="
                  data-sizes="auto"
                  loading="lazy"
                  data-src="{{ collectionLiquid.image.src | img_url: 'large' | replace: '_large', imgLz }}"
                  width="{{ collectionLiquid.image.width }}"
                  height="{{ collectionLiquid.image.height }}"
                  alt="{{ collectionLiquid.title | escape }}"
                  data-widths="[40, 100, 140, 180, 250, 260, 275, 305, 440, 610, 720, 930, 1080]"
                  class="gmlazyload gm-product-img{% if class %} {{ class }}{% endif %}"
                >
