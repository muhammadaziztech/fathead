        </div>
      {% elsif menuItem.submenu.type %}
        <ul class="gm-{{ menuItem.submenu.type }}">
          {% for item in menuItem.submenu.items %}
            {% render 'globo.menu.item', menuItem: item %}
          {% endfor %}
        </ul>
      {% endif %}
    {% endif %}
  </li>
{% endcase %}
{% endif %}