            <p class="form-message form-message--success" data-form-status="">
              {% assign thanksPlaceholder = menuItem.thanks | default: settings.language.thanks %}
              {% if languageCode != "" %}
                {% assign thanksLanguageCode = 'thanks' | append: languageCode %}
                {% assign thanksPlaceholder = menuItem[thanksLanguageCode] %}
              {% endif %}
              {{ thanksPlaceholder }}
            </p>
          {% endif %}
