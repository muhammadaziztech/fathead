{% style %}
  {% if section.settings.section_top_margin %}
    #shopify-section-{{ section.id }}{
      margin-top: 30px;
    }
  {% endif %}
  {% if section.settings.section_bottom_margin %}
    #shopify-section-{{ section.id }} {
      margin-bottom: 30px;
    }
  {% endif %}
{% endstyle %}
{% render 'content.5d76937b643140ddbb31a4966a228d30.builder' %}

