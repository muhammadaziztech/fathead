<div
  class="custom_slideshow container container--no-mobile-padding large-row full-width-slideshow full-width-background relative section-{{ section.id }}"
  data-section-id="{{ section.id }}"
  data-section-type="slideshow">
  {% style %}
    {%- if section.settings.height == 'fixed' -%}
      @media(min-width: 1440px) {
        .section-{{ section.id }} .rimage-outer-wrapper
        , .section-{{ section.id }}
        .placeholder-container {
          height: {{ section.settings.height_large_desktop }}
          px !important;
        }
      }
      @media(min-width: 1024px) {
        .section-{{ section.id }} .rimage-outer-wrapper
        , .section-{{ section.id }}
        .placeholder-container {
          height: {{ section.settings.height_desktop }}px;
        }
      }
      @media(max-width: 1023.98px) {
        .section-{{ section.id }} .rimage-outer-wrapper
        , .section-{{ section.id }}
        .placeholder-container {
          height: {{ section.settings.height_small_desktop }}px;
        }
      }
      @media (min-width: 768px) {
        .section-{{ section.id }} .overlay {
          padding: 50px;
        }
      }
      @media(max-width: 767.98px) {
        .section-{{ section.id }} .rimage-outer-wrapper
        , .section-{{ section.id }}
        .placeholder-container {
          height: {{ section.settings.height_tablet }}px;
        }
      }
      @media(max-width: 550px) {
        .section-{{ section.id }} .rimage-outer-wrapper
        , .section-{{ section.id }}
        .placeholder-container {
          height: {{ section.settings.height_mobile }}px;
        }
      }
    {%- endif -%}

    {% for block in section.blocks %}
      .section-{{ section.id }}
      .block-{{ forloop.index }}
      .overlay {
        top: {{ block.settings.overlay_position_y }}%;
        left: {{ block.settings.overlay_position_x }}%;
      }
      .section-{{ section.id }}
      .block-{{ forloop.index }}
      .overlay-text {
        margin: 5px 0;
      }

      .section-{{ section.id }}
      .block-{{ forloop.index }}
      .overlay__links a {
        margin: 10px 7.5px;
      }
      .section-{{ section.id }}
      .block-{{ forloop.index }}
      .image-product-label-1 {
        top: {{ block.settings.label_position_y_1 }}%;
        left: {{ block.settings.label_position_x_1 }}%;
      }
      .section-{{ section.id }}
      .block-{{ forloop.index }}
      .image-product-label-2 {
        top: {{ block.settings.label_position_y_2 }}%;
        left: {{ block.settings.label_position_x_2 }}%;
      }
      .section-{{ section.id }} .block-{{ forloop.index }} .rimage-outer-wrapper {
        background-image: url('{{ block.settings.image | img_url: '1024x1024' }}');
        background-size: cover;
        background-position: {{ block.settings.image.presentation.focal_point }};
        position: relative;
      }
      @media (max-width: 767.98px) {
        .section-{{ section.id }} .block-{{ forloop.index }} .rimage-outer-wrapper {
          {% if block.settings.mobile_image !=blank %}
            background-image: url('{{ block.settings.mobile_image | img_url: '1024x1024' }}') !important;
          {% endif %}
        }
      }
      @media (min-width: 769px) and (max-width: 1199px) {
        .section-{{ section.id }}.block-{{ forloop.index }}
        .rimage-outer-wrapper
        , .section-{{ section.id }}
        .block-{{ forloop.index }}
        .placeholder-container {
          height: calc({{ section.settings.height_desktop }}px / 1.15);
        }
        .section-{{ section.id }}
        .block-{{ forloop.index }}
        .tp-heading {
          font-size: 22px;
        }
        .section-{{ section.id }}
        .block-{{ forloop.index }}
        .overlay-title {
          font-size: 50px;
        }
        .section-{{ section.id }}
        .block-{{ forloop.index }}
        .overlay-text {
          font-size: 16px;
        }
      }
    {% endfor %}

    .section-{{ section.id }}
    .overlay-text {
      line-height: 1.3;
      font-size: 18px;
    }
    .tp-heading {
      font-size: 24px;
      font-weight: 500;
    }
    .section-{{ section.id }}
    .overlay-title {
      font-size: 55px;
    }
    .section-{{ section.id }}
    .full-width-slideshow-controls {
      position: absolute;
      top: 43%;
      z-index: 2;
      color: #000;
      background: #fff;
      border: 0 solid rgba(0, 0, 0, 0.1);
      border-radius: 100%;
      display: flex;
      align-items: center;
      justify-content: center;
      height: 55px;
      width: 55px;
      box-shadow: 0 0 5px #00000059;
      transition: all 0.3s ease-in;
    }
    .section-{{ section.id }}
    .full-width-slideshow-controls--prev {
      left: 20px;
    }
    .section-{{ section.id }}
    .full-width-slideshow-controls--next {
      right: 20px;
    }
    .section-{{ section.id }}
    .full-width-slideshow-controls .icon {
      height: 25px;
      width: 26px;
      align-items: center;
      display: flex;
      justify-content: center;
    }
    .section-{{ section.id }}.slick-dots li.slick-active button:before {
      color: #e50105;
    }
    .section-{{ section.id }}
    .slick-dots {
      bottom: 20px;
      position: absolute;
      width: 100%;
    }
    .section-{{ section.id }}.slick-dots li button:before {
      font-size: 10px;
      opacity: 1;
      color: #fff;
    }
    .section-{{ section.id }}
    .slick-dots li button {
      font-size: 0;
      background: unset;
      border: unset;
      height: unset;
    }
    .custom_slideshow {
      margin-top: 0;
    }
    .overlay-img {
      position: absolute;
      right: 0;
      bottom: 0;
      height: 100%;
      z-index: 1;
    }
    .section-{{ section.id }}
    .overlay-img-mobile {
      display: none;
    }
    @media(max-width: 767.98px) {
      .section-{{ section.id }} .overlay {
        padding: 20px 15px;
        width: 100%;
        top: 0 !important;
        left: 0 !important;
        right: 0 !important;
        margin-top: 20px !important;
        margin-left: 20px !important;
        margin-right: 20px !important;
      }
      .section-{{ section.id }}
      .overlay-standard.mobile-overlay-under {
        transform: unset;
      }
      .section-{{ section.id }}
      .overlay-title {
        font-size: 38px;
        margin-bottom: 6px;
      }
      .section-{{ section.id }}
      .overlay-text {
        font-size: 15px;
        line-height: 1.1;
      }
      .section-{{ section.id }}
      .tp-heading {
        font-size: 18px;
      }
      .section-{{ section.id }}
      .overlay__bg {
        opacity: 0.7 !important;
      }
      .first-c-logo {
        left: 2% !important;
        bottom: 5% !important;
        max-width: 150px !important;
        transform: unset !important;
      }
      .second-br-logo {
        bottom: 5% !important;
        max-width: 120px !important;
      }
      .section-{{ section.id }}
      .full-width-slideshow-controls {
        top: 50% !important;
        transform: tranlateY(-50%);
      }
      .section-{{ section.id }}
      .btn {
        margin: 0;
        font-size: 14px;
        padding: 8px 25px;
      }
      .section-{{ section.id }}
      .overlay-img-desktop {
        display: none;
      }
      .section-{{ section.id }}
      .overlay-img-mobile {
        display: block;
      }
      .section-{{ section.id }}
      .overlay-img {
        height: auto;
        bottom: 0;
        max-width: 100%;
      }
    }
    {% if section.settings.section_top_margin %}
      #shopify-section-{{ section.id }} {
        margin-top: 30px;
      }
    {% endif %}
    {% if section.settings.section_bottom_margin %}
      #shopify-section-{{ section.id }} {
        margin-bottom: 30px;
      }
    {% endif %}
  {% endstyle %}

  <div class="page-width relative">
    <div
      class="full-width relative js-slideshow-section"
      data-transition="{{ section.settings.slide_transition }}"
      data-autoplay="{{ section.settings.slideshow_autoplay }}"
      data-autoplayspeed="{{ section.settings.slideshow_autoplay_speed }}000">
      {% for block in section.blocks %}
        {%- if section.settings.height == 'adapt' -%}
          {%- assign aspect_ratio = section.blocks.first.settings.image.aspect_ratio | default: 2.2 -%}
        {%- else -%}
          {%- assign aspect_ratio = block.settings.image.aspect_ratio -%}
        {%- endif -%}

        <div class="slide container-display-wrapper relative avoid-overlaps block-{{ forloop.index }}" {{ block.shopify_attributes }}>
          <div
            class="avoid-overlaps__mobile-container"
            {% if forloop.first %}
            data-cc-animate="zoom-out"
            {% endif %}>
            {% if block.settings.image_link != blank %}<a href="{{ block.settings.image_link }}">{% endif %}

            {% if block.settings.image == blank %}
              {% capture placeholder_name %}lifestyle-{{ forloop.index | modulo: 2 | plus: 1 }}{% endcapture %}
              <div
                class="placeholder-container relative"
                {%- if section.settings.height=='adapt' -%}
                style="padding-top:{{ 1 | divided_by: aspect_ratio | times: 100 }}%;"
                {%- endif -%}>
                {{ placeholder_name | placeholder_svg_tag: 'placeholder-svg' }}
              </div>
            {% else %}
              <div
                class="rimage-outer-wrapper rimage-background lazyload fade-in"
                data-bgset="{% render 'bgset', image: block.settings.image %}"
                data-sizes="auto"
                data-parent-fit="cover">
                <div class="rimage-wrapper" style="padding-top:{{ 1 | divided_by: aspect_ratio | times: 100 }}%">
                  <noscript>
                    <img
                      src="{{ block.settings.image | img_url: '1024x1024' }}"
                      alt="{{ block.settings.image.alt | escape }}"
                      class="rimage__image"
                      width="width"
                      height="height">
                  </noscript>
                </div>

                {% if block.settings['first-logo'] != blank %}
                  <img
                    class="first-c-logo {% unless forloop.first %}lazyload fade-in{% endunless %}"
                    {% if forloop.first %}
                    src="{{ block.settings.first-logo | img_url: 'original' }}"
                    {% else %}
                    data-src="{{ block.settings.first-logo | img_url: 'original' }}"
                    {% endif %}
                    alt="{{ block.settings.first-logo.alt }}"
                    width="width"
                    height="height"
                    style="max-width:{{ block.settings.first_logo_width }}px; height:auto; position: absolute; bottom: 50%; left: 48%; z-index:2; transform: translateY(50%);">
                {% endif %}
                {% if block.settings['second-logo'] != blank %}
                  <img
                    class="second-br-logo {% unless forloop.first %}lazyload fade-in{% endunless %}"
                    {% if forloop.first %}
                    src="{{ block.settings.second-logo | img_url: 'original' }}"
                    {% else %}
                    data-src="{{ block.settings.second-logo | img_url: 'original' }}"
                    {% endif %}
                    alt="{{ block.settings.second-logo.alt }}"
                    width="width"
                    height="height"
                    style="max-width:{{ block.settings.second_logo_width }}px; height:auto; position: absolute; bottom:10%; right:5%; z-index:2; ">
                {% endif %}

                {% if block.settings.overlay-img != blank %}
                  <img
                    class="overlay-img-desktop overlay-img {% unless forloop.first %}lazyload fade-in{% endunless %}"
                    {% if forloop.first %}
                    src="{{ block.settings.overlay-img |img_url: 'original' }}"
                    {% else %}
                    data-src="{{ block.settings.overlay-img |img_url: 'original' }}"
                    {% endif %}
                    alt="{{ block.settings.overlay-img.alt }}"
                    width="width"
                    height="height">
                {% endif %}

                {% if block.settings.overlay-img-mobile != blank %}
                  <img
                    class="overlay-img-mobile overlay-img {% unless forloop.first %}lazyload fade-in{% endunless %}"
                    {% if forloop.first %}
                    src="{{ block.settings.overlay-img-mobile |img_url: 'original' }}"
                    {% else %}
                    data-src="{{ block.settings.overlay-img-mobile |img_url: 'original' }}"
                    {% endif %}
                    alt="{{ block.settings.overlay-img.alt }}"
                    width="width"
                    height="height">
                {% endif %}
              </div>
            {% endif %}

            {% if block.settings.image_link != blank %}
              </a>{% endif %}

            {% for i in (1..2) %}
              {% capture current_number %}{{ i }}{% endcapture %}
              {% capture current_label %}product_id_{{ current_number }}{% endcapture %}
              {% capture chevron_position %}chevron_position_{{ current_number }}{% endcapture %}
              {% if block.settings[current_label] != blank %}
                {% assign product_label = all_products[block.settings[current_label]] %}
                {% assign quickbuy_toggle = settings.quickbuy_labels_toggle %}
                {% render 'image-product-label'
                  , product_label: product_label
                  , current_number: current_number
                  , show_chevron: block.settings.show_chevron
                  , chevron: block.settings[chevron_position]
                  , quickbuy: quickbuy_toggle
                  , color_bg: block.settings.color_label_background
                  , color_text: block.settings.color_label_text
                %}
              {% endif %}
            {% endfor %}
          </div>

          {% render 'image-overlay'
            , background_color: block.settings.color_overlay_bg
            , background_opacity: block.settings.overlay_opacity
            , text_color: block.settings.color_overlay_text
            , heading: block.settings.overlay_heading
            , title: block.settings.overlay_title
            , enlarge_title: block.settings.enlarge_title
            , title_width: block.settings.overlay_title_width
            , text: block.settings.overlay_text
            , enlarge_text: block.settings.enlarge_text
            , url: block.settings.overlay_url_1
            , url_text: block.settings.overlay_url_text_1
            , button_bg_1: block.settings.overlay_button_bg_1
            , url_2: block.settings.overlay_url_2
            , url_text_2: block.settings.overlay_url_text_2
            , button_bg_2: block.settings.overlay_button_bg_2
            , url_style: block.settings.overlay_url_style
            , mobile_text_below: section.settings.mobile_text_below
            , extra_large_text: true
            , text_alignment: block.settings.text_alignment
            , styled_words: block.settings.styled_words
            , words_style: block.settings.words_style
            , styled_word_color: block.settings.styled_word_color
            , highlight_color: block.settings.highlight_color
            , highlight_words_top: block.settings.highlight_words_top
          %}

        </div>
      {% endfor %}
    </div>

    {% if section.blocks.size > 1 %}
      <div class="full-width-slideshow-controls full-width-slideshow-controls--prev js">
        <button class="btn--plain prev icon feather-icon btn-arrow-prev" aria-label="{{ 'general.accessibility_labels.previous' | t | escape }}">
          {% render 'custom-slide-arrow-left' %}
        </button>
      </div>
      <div class="full-width-slideshow-controls full-width-slideshow-controls--next js btn-arrow-next">
        <button class="btn--plain next icon feather-icon" aria-label="{{ 'general.accessibility_labels.next' | t | escape }}">
          {% render 'custom-slide-arrow-right' %}
        </button>
      </div>
    {% endif %}
  </div>
</div>

{% schema %}
  {
    "name": "FH - Slideshow",
    "tag": "section",
    "max_blocks": 10,
    "settings": [
      {
        "type": "checkbox",
        "id": "section_top_margin",
        "label": "Add section top margin",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "section_bottom_margin",
        "label": "Add section bottom margin",
        "default": false
      },
      {
        "type": "radio",
        "id": "height",
        "label": "Image height",
        "options": [
          {
            "value": "adapt",
            "label": "Adapt to first image"
          }, {
            "value": "fixed",
            "label": "Fixed height"
          }
        ],
        "default": "adapt"
      },
      {
        "type": "range",
        "id": "height_large_desktop",
        "min": 200,
        "max": 1000,
        "step": 10,
        "unit": "px",
        "label": "Larger Desktop fixed height",
        "default": 490
      }, {
        "type": "range",
        "id": "height_desktop",
        "min": 200,
        "max": 1000,
        "step": 10,
        "unit": "px",
        "label": "Desktop fixed height",
        "default": 400
      }, {
        "type": "range",
        "id": "height_small_desktop",
        "min": 200,
        "max": 1000,
        "step": 10,
        "unit": "px",
        "label": "Smaller desktop fixed height",
        "default": 400
      }, {
        "type": "range",
        "id": "height_tablet",
        "min": 100,
        "max": 1000,
        "step": 10,
        "unit": "px",
        "label": "Tablet fixed height",
        "default": 390
      }, {
        "type": "range",
        "id": "height_mobile",
        "min": 100,
        "max": 1000,
        "step": 10,
        "unit": "px",
        "label": "Mobile fixed height",
        "default": 350
      }, {
        "type": "checkbox",
        "id": "mobile_text_below",
        "label": "Show overlay below image on mobile",
        "info": "If not ticked product labels may not show on mobile",
        "default": true
      }, {
        "type": "header",
        "content": "Slideshow"
      }, {
        "type": "select",
        "id": "slide_transition",
        "label": "Transition",
        "options": [
          {
            "value": "zoom",
            "label": "Zoom"
          }, {
            "value": "fade",
            "label": "Fade"
          }, {
            "value": "slide",
            "label": "Slide"
          }, {
            "value": "slide_fade",
            "label": "Slide & Fade"
          }
        ],
        "default": "fade"
      }, {
        "type": "checkbox",
        "id": "slideshow_autoplay",
        "label": "Auto rotate between slides",
        "default": true
      }, {
        "type": "range",
        "id": "slideshow_autoplay_speed",
        "label": "Change slides every",
        "min": 4,
        "max": 10,
        "unit": "s",
        "step": 1,
        "default": 6
      }
    ],
    "blocks": [
      {
        "type": "image",
        "name": "Slide",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image",
            "info": "2048 x 1024px recommended"
          },
          {
            "type": "image_picker",
            "id": "mobile_image",
            "label": "Image (Mobile)"
          },
          {
            "type": "url",
            "id": "image_link",
            "label": "Image link"
          },
          {
            "type": "image_picker",
            "id": "first-logo",
            "label": "Center Logo"
          }, {
            "type": "range",
            "id": "first_logo_width",
            "label": "Center logo width",
            "max": 400,
            "min": 50,
            "step": 5,
            "unit": "px",
            "default": 200
          }, {
            "type": "image_picker",
            "id": "second-logo",
            "label": "Bottom Right Logo"
          }, {
            "type": "range",
            "id": "second_logo_width",
            "label": "Bottom right logo width",
            "max": 400,
            "min": 50,
            "step": 5,
            "unit": "px",
            "default": 140
          }, {
            "type": "header",
            "content": "Overlay - layout"
          }, {
            "type": "image_picker",
            "id": "overlay-img",
            "label": "Overlay Image (Desktop)"
          }, {
            "type": "image_picker",
            "id": "overlay-img-mobile",
            "label": "Overlay Image (Mobile)"
          }, {
            "type": "color",
            "id": "color_overlay_bg",
            "label": "Background color",
            "default": "#ffffff"
          }, {
            "type": "range",
            "id": "overlay_opacity",
            "min": 0,
            "max": 100,
            "step": 1,
            "label": "Background opacity",
            "default": 80,
            "unit": "%",
            "info": "Does not apply when the overlay is shown under the image on mobile."
          }, {
            "type": "color",
            "id": "color_overlay_text",
            "label": "Text color",
            "default": "#333333"
          }, {
            "type": "radio",
            "id": "text_alignment",
            "label": "Text alignment",
            "options": [
              {
                "value": "center",
                "label": "Center"
              },
              {
                "value": "align-left",
                "label": "Left"
              },
              {
                "value": "align-right",
                "label": "Right"
              },
              {
                "value": "align-left-desktop-only",
                "label": "Left on desktop only"
              }, {
                "value": "align-right-desktop-only",
                "label": "Right on desktop only"
              }
            ]
          }, {
            "type": "range",
            "id": "overlay_position_x",
            "label": "Horizontal position",
            "min": 0,
            "max": 100,
            "unit": "%",
            "step": 1,
            "default": 26
          }, {
            "type": "range",
            "id": "overlay_position_y",
            "label": "Vertical position",
            "min": 0,
            "max": 100,
            "unit": "%",
            "step": 1,
            "default": 80
          }, {
            "type": "header",
            "content": "Overlay - heading"
          }, {
            "type": "text",
            "id": "overlay_heading",
            "label": "Heading"
          }, {
            "type": "text",
            "id": "overlay_title",
            "label": "Heading"
          }, {
            "type": "checkbox",
            "id": "enlarge_title",
            "label": "Enlarge Title",
            "default": false
          }, {
            "type": "range",
            "id": "overlay_title_width",
            "label": "Heading width",
            "min": 50,
            "max": 100,
            "unit": "%",
            "step": 1,
            "default": 100
          }, {
            "type": "text",
            "id": "styled_words",
            "label": "Words to style",
            "info": "Case-sensitive."
          }, {
            "type": "select",
            "id": "words_style",
            "label": "Word style",
            "options": [
              {
                "value": "stencil",
                "label": "Stencil"
              }, {
                "value": "highlight",
                "label": "Highlight"
              }
            ],
            "default": "stencil"
          }, {
            "type": "range",
            "id": "highlight_words_top",
            "label": "Highlight vertical position",
            "info": "Use to fine tune the placement of the highlight. This may vary from font to font.",
            "min": -5,
            "max": 20,
            "unit": "%",
            "step": 1,
            "default": 10
          }, {
            "type": "color",
            "id": "highlight_color",
            "label": "Highlight color",
            "default": "#ffffff"
          }, {
            "type": "color",
            "id": "styled_word_color",
            "label": "Word color",
            "default": "#b5e0fa"
          }, {
            "type": "header",
            "content": "Overlay - text"
          }, {
            "type": "text",
            "id": "overlay_text",
            "label": "Text",
            "default": "Use this section to welcome customers to your store, show off your images or display a promotion"
          }, {
            "type": "checkbox",
            "id": "enlarge_text",
            "label": "Enlarge text",
            "default": false
          }, {
            "type": "header",
            "content": "Overlay - buttons"
          }, {
            "type": "text",
            "id": "overlay_url_text_1",
            "label": "Button 1 text"
          }, {
            "type": "url",
            "id": "overlay_url_1",
            "label": "Button 1 link"
          }, {
            "type": "color",
            "id": "overlay_button_bg_1",
            "label": "Button 1 background",
            "default": "#d43900"
          }, {
            "type": "text",
            "id": "overlay_url_text_2",
            "label": "Button 2 text"
          }, {
            "type": "url",
            "id": "overlay_url_2",
            "label": "Button 2 link"
          }, {
            "type": "color",
            "id": "overlay_button_bg_2",
            "label": "Button 2 background",
